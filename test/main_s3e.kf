// подгрузка библиотек
 " proteus.lib" L
 " wait50MHz.lib" L
 " lcd.lib" L

: BaseAddr 0x8000 ; INLINE
: DataPin! BaseAddr OUTPORT ; INLINE // 1/0 -->

MAIN:
  0
BEGIN
  DUP DataPin! 1 +
  HOME SEC HEXPRINT
  WAIT_1s
AGAIN
