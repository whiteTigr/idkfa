 ScanCodes @ 0x59 = IF 1  rCtrl ! ; THEN
 ScanCodes @ 0x11 = IF 1   lAlt ! ; THEN
 ScanCodes @ 0x59 = IF 1   rAlt ! ; THEN
 DROP 0
;

