  IF 1023 ELSE 0 THEN 3001 OUTPORT
  CapsLock ! 
 ; THEN

 ScanCodes @ 0x77 = IF    
  NumLock @ INVERT DUP
  IF 1023 ELSE 0 THEN 3002 OUTPORT
  NumLock ! 
 ; THEN 

 DROP 0
;

: isSpecDown
 1
