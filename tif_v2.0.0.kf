  IF
   1
   ScanCodes @ 0x11 = IF 1  rAlt ! ; THEN
   ScanCodes @ 0x14 = IF 1 rCtrl ! ; THEN
   0 tmpCount !
   0 KEYDATA !
   BEGIN
    E0symScan tmpCount @ + @ ScanCodes @ = IF
        E0sym tmpCount @ + @ KEYDATA !
    THEN
    tmpCount @ 1 + tmpCount !
   KEYDATA @ tmpCount @ 15 > OR UNTIL
   KEYDATA @ 0 = IF DROP 0 ; THEN
   1 KeyDown !   
  ELSE
   0
  THEN 
;

: isSpecUp
