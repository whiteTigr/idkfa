 ScanCodes @ 0x14 = IF 0  lCtrl ! ; THEN
 ScanCodes @ 0x59 = IF 0  rCtrl ! ; THEN
 ScanCodes @ 0x11 = IF 0   lAlt ! ; THEN
 ScanCodes @ 0x59 = IF 0   rAlt ! ; THEN

